        "lint-staged": "^10.5.1",
        "nodemon": "^2.0.6",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.1.2",
        "pretty-quick": "^2.0.2",
        "prop-types": "^15.7.2",
        "terser-webpack-plugin": "^5.0.3",
        "webpack": "^5.4.0",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.0.2",
        "webpack-merge": "^5.4.0",
        "webpack-node-externals": "^2.5.2"
    }
}
